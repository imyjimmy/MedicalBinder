{
  "name": "MedicalBinder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:simulator": "CONFIGURATION=Debug react-native run-ios --simulator=\"iPhone 15 Pro\"",
    "dev": "react-native start --reset-cache",
    "build:ios": "expo run:ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "yarn clean:metro && yarn clean:ios && yarn clean:modules",
    "clean:metro": "npx react-native start --reset-cache || true",
    "clean:ios": "cd ios && rm -rf build/ && rm -rf Pods/ && rm -f Podfile.lock && cd ..",
    "clean:modules": "rm -rf node_modules/ && yarn install",
    "clean:rebuild": "yarn clean && cd ios && pod install && cd .. && yarn ios",
    "clean:full": "yarn clean:modules && yarn clean:ios && cd ios && pod install && cd .. && yarn ios:simulator"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.13",
    "@react-navigation/stack": "^7.3.6",
    "expo": "^53.0.11",
    "expo-cli": "^6.3.10",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.26.0",
    "react-native-mgit": "file:../react-native-mgit",
    "react-native-screens": "^4.11.1",
    "react-native-vision-camera": "^4.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.3",
    "@react-native/eslint-config": "0.79.3",
    "@react-native/metro-config": "0.79.3",
    "@react-native/typescript-config": "0.79.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-safe-area-context": "^5.4.1",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
