{
  "name": "MedicalBinder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:sim": "CONFIGURATION=Debug react-native run-ios --simulator=\"iPhone 15 Pro\"",
    "dev": "react-native start --reset-cache",
    "build:ios": "expo run:ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "yarn clean:metro && yarn clean:ios && yarn clean:modules",
    "clean:metro": "npx react-native start --reset-cache || true",
    "clean:metro-cache-only": "rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/react-* && rm -rf node_modules/.cache",
    "clean:ios": "cd ios && rm -rf build/ && rm -rf Pods/ && rm -f Podfile.lock && cd ..",
    "clean:modules": "rm -rf node_modules/ && yarn install",
    "clean:rebuild": "yarn clean && cd ios && pod install && cd .. && yarn ios",
    "clean:full": "yarn clean:metro-cache-only && yarn clean:modules && yarn clean:ios && cd ios && pod install && cd .. && yarn ios:sim",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@getalby/sdk": "^5.1.1",
    "@noble/secp256k1": "^2.0.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.20",
    "bech32": "^2.0.0",
    "crypto-js": "^4.2.0",
    "expo": "^53.0.11",
    "expo-cli": "^6.3.10",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-dotenv": "^3.4.11",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.26.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keychain": "^8.2.0",
    "react-native-mgit": "file:../react-native-mgit",
    "react-native-screens": "^4.11.1",
    "react-native-shared-element": "^0.8.9",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vision-camera": "^4.6.4",
    "react-navigation-shared-element": "^3.1.3",
    "text-encoding-polyfill": "^0.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.3",
    "@react-native/eslint-config": "0.79.3",
    "@react-native/metro-config": "0.79.3",
    "@react-native/typescript-config": "0.79.3",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-safe-area-context": "^5.4.1",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
